    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["00:1A:22:0C:19:60", "--lock", "off", "--dump", "--print"]
        }
    ]
}